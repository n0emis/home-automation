auth                        86400	IN	CNAME	elara.het.nbg.de.n0emis.eu.
keycloak                    86400	IN	CNAME	elara.het.nbg.de.n0emis.eu.

md                          86400   IN  CNAME   int.goe.de.dyn.n0emis.eu.
